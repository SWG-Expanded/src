CREATE TABLE "LOG_TYPE" (
 "LOG_TYPE_ID" VARCHAR2(10) NOT NULL, 
 "LOG_TYPE_DESC" VARCHAR2(255) NOT NULL );

CREATE TABLE LOGGER ( 
  LOGTIME      DATE          DEFAULT SYSDATE NOT NULL, 
  LOG_TYPE_ID  VARCHAR2 (10)  NOT NULL, 
  MSG_ID       NUMBER (10)   NOT NULL, 
  MSG_TEXT     VARCHAR2 (255)  NOT NULL);

