CREATE TABLE ERRORLOG_RERAISE_LIMITS ( 
PACKAGE VARCHAR2(30) NOT NULL, 
PROCEDURE VARCHAR2(30) NOT NULL,
ERRORLIMIT NUMBER(10) DEFAULT 0 NOT NULL, 
ERRORCOUNT NUMBER(10) DEFAULT 0 NOT NULL, 
UNLIMITED_FLAG NUMBER(10) DEFAULT 0 NOT NULL,
CONSTRAINT PK_RERAISE_LIMITS PRIMARY KEY(PACKAGE, PROCEDURE)
);

grant select on errorlog_reraise_limits to public;

